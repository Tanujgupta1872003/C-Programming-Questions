// Write a program to receive value of an angle in degrees and check whether sum of squares of sine and cosine of this angle is equal to 1.
#include<stdio.h>
#include<conio.h>
#include<math.h>
void main()
{
    float angle,result;

    printf("Enter Value of Angle: ");
    scanf("%f",&angle);

    result = (sin(angle)*sin(angle) + cos(angle)*cos(angle));
    printf("Result is: %f", result);
getch();
}
