// The marks obtained by a student in 5 different subjects are input through the keyboard. Write a program to calculate the division obtained by the student.
#include<stdio.h>
#include<conio.h>
void main()
{
    float m1,m2,m3,m4,m5,per;
    printf("Enter First Sub Marks: ");
    scanf("%f",&m1);

    printf("Enter Second Sub Marks: ");
    scanf("%f",&m2);
    
    printf("Enter Third Sub Marks: ");
    scanf("%f",&m3);
    
    printf("Enter Fourth Sub Marks: ");
    scanf("%f",&m4);
    
    printf("Enter Fifth Sub Marks: ");
    scanf("%f",&m5);

    per = (m1 + m2 + m3 + m4 + m5)*100/500;
    printf("Percentage Gained = %f\n",per);

    if (per>=60){
        printf("First division\n");
    }
    else
    {
        if (per>= 50)
            printf("Second division\n") ;
        else
        {
            if (per>=40)
                printf("Third division\n") ;
            else
                printf("Fail\n");
        }
    }
return 0;
}
